<div class="uk-container uk-container-center uk-margin-top">

    <img class="uk-border-circle" width="120" height="120" src="{{.Data.Repo.Owner.AvatarURL}}" alt="">

    <h3><a href="{{.Data.Repo.HTMLURL}}">{{.Data.Repo.FullName}}</a></h3>

    <div>
        {{.Data.Commit.Commit.Message}}
    </div>
    <div>
        {{.Data.Commit.SHA}}
    </div>
    <pre>{{.Data.File}}</pre>

    {{ range $key, $value := .Data.Deploy }}
        <a class="uk-button uk-button-success uk-margin-top" href="/repos/{{$.Data.Repo.Owner.Login}}/{{$.Data.Repo.Name}}/commit/{{$.Data.Commit.SHA}}/run/{{$key}}">{{$key}}</a>
    {{end}}

</div>